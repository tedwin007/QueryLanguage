{
  "name": "query-language",
  "version": "1.0.1",
  "description": "Query Language",
  "main": "main/index.js",
  "scripts": {
    "test": "jasmine --config=jasmine.json",
    "coverage": "nyc npm run test && nyc report --reporter=html",
    "generate-types": "npx typeorm-model-generator",

    "dev": "nodemon src/index.ts",
    "tsc": "tsc -p ./",
    "doc": "typedoc --tsconfig tsconfig.json"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "mssql": "^9.0.1",
    "mysql": "^2.18.1",
    "oracledb": "^5.5.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "sql.js": "^1.8.0",
    "sqlite3": "^5.1.4",
    "typeorm-model-generator": "^0.4.6",
    "uuidv4": "^6.2.6"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@types/express": "^4.17.11",
    "@types/jasmine": "^4.3.1",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.18.36",
    "ava": "^5.1.0",
    "chevrotain": "^10.4.2",
    "eslint": "^7.8.0",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "prettier": "^2.1.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typedoc": "^0.23.22",
    "typeorm": "^0.3.11",
    "typescript": "4.9"
  },
  "files": [
    "main",
    "!**/*.json",
    "README.md"
  ],
  "ava": {
    "failFast": true,
    "timeout": "60s",
    "typescript": {
      "rewritePaths": {
        "src/": "build/main/"
      }
    },
    "files": [
      "!build/module/**"
    ]
  }
}
