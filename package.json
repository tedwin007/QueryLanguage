{
  "name": "filters-manger",
  "version": "1.2.0",
  "description": "Filter Manger",
  "main": "main/index.js",
  "scripts": {
    "test": "jasmine --config=jasmine.json",
    "coverage": "nyc npm run test && nyc report --reporter=html",
    "dev": "nodemon src/index.ts",
    "tsc": "tsc -p ./",
    "doc": "typedoc --tsconfig tsconfig.json"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "uuidv4": "^6.2.6"
  },
  "devDependencies": {
    "chevrotain": "^10.4.2",
    "@ava/typescript": "^1.1.1",
    "@types/express": "^4.17.11",
    "@types/jasmine": "^4.3.1",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.31",
    "ava": "^5.1.0",
    "eslint": "^7.8.0",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "prettier": "^2.1.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typedoc": "^0.23.22",
    "typescript": "4.9"
  },
  "files": [
    "main",
    "!**/*.json",
    "README.md"
  ],
  "ava": {
    "failFast": true,
    "timeout": "60s",
    "typescript": {
      "rewritePaths": {
        "src/": "build/main/"
      }
    },
    "files": [
      "!build/module/**"
    ]
  }
}
